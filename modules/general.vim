colorscheme xoria256
let mapleader=","
syntax on
set directory=/tmp//
set clipboard=unnamed
set hidden
set modelines=0
set exrc
set relativenumber
set numberwidth=2
set nofoldenable
set sidescroll=1
set shell=/bin/bash
set updatetime=100

let $NVIM_TUI_ENABLE_CURSOR_SHAPE = 0
set guicursor=
set mouse=a
